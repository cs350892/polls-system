{ 
  "compilerOptions": { 
    "target": "es6", 
    "module": "commonjs", 
    "strict": true, 
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "outDir": "dist", 
    "types": ["node", "express", "cors"]
  }, 
  "include": ["src/**/*"], 
  "exclude": ["node_modules"] 
}